<!-- Content -->
<div class="container">
  <h1 class="text-center">Halaman Contact</h1>
  <h4>Data Contact</h4>
  <!-- Cek Jika data Kosong -->
  <% if (cont.length <1) { %>
    <div class="alert alert-danger" role="alert">Data Empty</div>
  <% } else { %>
    <table class="table table-info">
      <thead>
        <tr>
          <th scope="col">No</th>
          <th scope="col">Nama</th>
          <th scope="col">Email</th>
        </tr>
      </thead>
      <tbody>
        <% cont.forEach((contact,i)=> { %>
        <tr>
          <th><%= i+1 %></th>
          <td><%= contact.nama %></td>
          <td><%= contact.email %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  <% } %>
</div>